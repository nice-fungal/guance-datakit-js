{
    "sourceMaps": true,
    "include": [
        "./src/**/*.js"
    ],
    "ignore": [
        "./src/**/*.spec.js"
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ]
    ],
    "plugins": [
        [
        "@babel/plugin-transform-runtime",
            {
                "helpers": true
            }
        ]
    ]
}